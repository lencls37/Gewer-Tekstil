.ps-home-banner
    .container
        .row
            +column({cols: [9, 12, 12, 12, 12]})
                .ps-carousel--dots.owl-slider(
                data-owl-auto= 'true',
                data-owl-loop= 'true',
                data-owl-speed= '5000',
                data-owl-gap= '0',
                data-owl-nav= 'false',
                data-owl-dots= 'true',
                data-owl-item= '1',
                data-owl-item-xs= '1',
                data-owl-item-sm= '1',
                data-owl-item-md= '1',
                data-owl-item-lg= '1',
                data-owl-duration= '1000',
                data-owl-mousedrag= 'on'
                )
                    a(href='#'): img(src="img/banner/home-9/1.jpg" alt="")
                    a(href='#'): img(src="img/banner/home-9/2.jpg" alt="")
            +column({cols: [3, 12, 12, 12, 12]})
                a(href='#').ps-block--collection: img(src="img/collection/home-9/1.jpg" alt="")
                a(href='#').ps-block--collection: img(src="img/collection/home-9/2.jpg" alt="")
